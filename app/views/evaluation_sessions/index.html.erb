<div class="page-header">

  <h3>Sesiuni de evaluare</h3>
</div>
<div class="well">
  <%= link_to 'Adauga o sesiune de evaluare ', new_evaluation_session_path, remote: true, class: 'btn btn-default' %>
</div>
<div class="new-session"></div>

<br/>
<div role="tabpanel">

  <!-- Nav tabs -->
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active">
      <a href="#terminal" aria-controls="terminal" role="tab" data-toggle="tab">Sesiuni de evaluare pentru ani
        terminali</a>
    </li>
    <li role="presentation">
      <a href="#neterminal" aria-controls="neterminal" role="tab" data-toggle="tab">Sesiuni de evaluare pentru ani
        neterminali</a></li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="terminal">

      <div class="table-responsive">

        <table class="table table-striped table-hover">
          <thead>
          <tr>
            <th></th>
            <th>Perioada de evaluare</th>
            <!--    <th>Chestionar</th>-->
            <th>Semestru</th>
            <th>An</th>
            <th>Status</th>

            <th>Vizibilitate Profesori</th>
            <th></th>
            <th></th>
          </tr>
          </thead>

          <tbody class="session-index">
          <%= render 'index' %>
          </tbody>
        </table>
      </div>
    </div>
    <div role="tabpanel" class="tab-pane" id="neterminal">
      <div class="table-responsive">

        <table class="table table-striped table-hover">
          <thead>
          <tr>
            <th></th>
            <th>Perioada de evaluare</th>
            <!--<th>Chestionar</th>-->
            <th>Semestru</th>
            <th>An</th>
            <th>Status</th>

            <th>Vizibilitate Profesori</th>
            <th></th>
            <th></th>
          </tr>
          </thead>
          <tbody class="session-index-neterminal">
          <%= render 'index2' %>
          </tbody>
        </table>
      </div>
    </div>
  </div>

</div>


<br>

<script type="text/javascript">
    $('#myTab a').click(function (e) {
        e.preventDefault()
        $(this).tab('show')
    })
</script>